<p id="notice"><%= notice %></p>

<h1>Garages</h1>

<table class="table table-hover table-sm">
  <thead>
    <tr class="thead-inverse">
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Website</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @garages.each do |garage| %>
      <tr>
        <td><%= garage.name %></td>
        <td><%= garage.address %></td>
        <td><%= garage.phone %></td>
        <td><%= garage.website %></td>
        <td><%= link_to 'Show', garage %></td>
        <td><%= link_to 'Edit', edit_garage_path(garage) %></td>
        <td><%= link_to 'Destroy', garage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Garage', new_garage_path %>
